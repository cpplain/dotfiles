{
  "bash-language-server": {
    "command": "bash-language-server",
    "args": ["start"],
    "extensionToLanguage": {
      ".sh": "shellscript",
      ".bash": "shellscript",
      ".zsh": "shellscript",
      ".ksh": "shellscript"
    }
  },
  "css-language-server": {
    "command": "vscode-css-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".css": "css",
      ".scss": "scss",
      ".less": "less"
    }
  },
  "gopls": {
    "command": "gopls",
    "extensionToLanguage": {
      ".go": "go"
    }
  },
  "html-language-server": {
    "command": "vscode-html-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".html": "html"
    }
  },
  "lua-language-server": {
    "command": "lua-language-server",
    "extensionToLanguage": {
      ".lua": "lua"
    }
  },
  "marksman": {
    "command": "marksman",
    "args": ["server"],
    "extensionToLanguage": {
      ".md": "markdown"
    }
  },
  "pyright": {
    "command": "pyright-langserver",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".py": "python",
      ".pyi": "python"
    }
  },
  "rust-analyzer": {
    "command": "rust-analyzer",
    "extensionToLanguage": {
      ".rs": "rust"
    }
  },
  "solargraph": {
    "command": "solargraph",
    "args": ["stdio"],
    "extensionToLanguage": {
      ".rb": "ruby",
      ".rake": "ruby",
      ".gemspec": "ruby"
    }
  },
  "sourcekit-lsp": {
    "command": "sourcekit-lsp",
    "extensionToLanguage": {
      ".swift": "swift"
    }
  },
  "taplo": {
    "command": "taplo",
    "args": ["lsp", "stdio"],
    "extensionToLanguage": {
      ".toml": "toml"
    }
  },
  "typescript-language-server": {
    "command": "typescript-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".ts": "typescript",
      ".tsx": "typescriptreact",
      ".js": "javascript",
      ".jsx": "javascriptreact",
      ".mts": "typescript",
      ".cts": "typescript",
      ".mjs": "javascript",
      ".cjs": "javascript"
    }
  },
  "yaml-language-server": {
    "command": "yaml-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".yaml": "yaml",
      ".yml": "yaml"
    }
  },
  "zls": {
    "command": "zls",
    "extensionToLanguage": {
      ".zig": "zig"
    }
  }
}
